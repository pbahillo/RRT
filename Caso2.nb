          RowBox[{"{", "}"}]}], ";", 
         RowBox[{"nrtx", "=", "0"}], ";", "\[IndentingNewLine]", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"arr", ">", "checkTime"}], ",", 
